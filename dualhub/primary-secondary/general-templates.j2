config adom/pkg/ppkg_hub
    config firewall policy
        edit 1
            set name "allow-all-sample"
            set srcintf "any"
            set dstintf "any"
            set action accept
            set srcaddr "all"
            set dstaddr "all"
            set schedule "always"
            set service "ALL"
            set nat enable
        next
    end
end

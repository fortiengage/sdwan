config system interface
    edit "<WC_FGT_PORT_NAME>"
        set ip "<WC_FGT_PORT_IP>" "<WC_FGT_PORT_IP_NETWORK_MASK>"
        set allowaccess ping fabric
        set ip-managed-by-fortiipam disable
    next
end

config system dhcp server
    edit 1
        set default-gateway "<WC_FGT_PORT_IP>"
        set netmask "<WC_FGT_PORT_IP_NETWORK_MASK>"
        set interface "port15"
        config ip-range
            edit 1
                set start-ip <WC_FG_DHCP_IP_START>
                set end-ip <WC_FG_DHCP_IP_END>
            next
        end
    next
end
